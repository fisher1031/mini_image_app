<div class="contents row">
  <%= form_with(url: search_messages_path, local: true, method: :get, class: "search-form") do |form| %>
  <%= form.text_field :keyword, placeholder: "投稿を検索する", class: "search-input" %>
  <%= form.submit "検索", class: "search-btn" %>
<% end %>

<% @messages.each do |message| %>
<%= link_to "#{message.name}", message_path(message.id), method: :get %>
  <div class="messages">
    <% message.images.each do |image| %>
<%= image_tag image, class: 'images' %>
<%# image.filename %>
    <% end %>
  </div>

    <%= link_to "作成日：#{message.created_at}", message_path(message.id), method: :get %>
    <% message.liked_users.count %>
    <h1><%= message.content %></h1>
<% end %>
</div>